<?xml version="1.0" encoding="UTF-8"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" xmlns:controller="http://playerstage.sourceforge.net/gazebo/xmlschema/#controller"   name="panda" >
  <xacro:property name="M_PI" value="3.1415926535897931" />
  <xacro:macro name="panda_arm" params="arm_id:='panda' description_pkg:='franka_description' connected_to:='' xyz:='0 0 0' rpy:='0 0 0' gazebo_plugin:=''">
    <xacro:unless value="${not connected_to}">
      <joint name="${arm_id}_joint_${connected_to}" type="fixed">
        <parent link="${connected_to}"/>
        <child link="${arm_id}_link0"/>
        <origin rpy="${rpy}" xyz="${xyz}"/>
      </joint>
    </xacro:unless>

    <xacro:property name="joint_damping" value="1.0"/>

    <link name="world" />
    <joint name="support_to_world" type="fixed">
      <parent link="world" />
      <child link="support" />
      <origin xyz="0.18 0.0 0.0" rpy="0.0 0.0 0.0" />
    </joint>
    
    <link name = "support" type="fixed">
        <visual>
        <geometry>
          <mesh filename="package://${description_pkg}/meshes/visual/support.stl" scale="0.72 1 0.72"/>
        </geometry>
        <material name="support_color">
		      <color rgba="${50/255} ${50/255} ${50/255} 1.0"/>
      	</material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://${description_pkg}/meshes/collision/support.stl" scale="0.72 1 0.72"/>
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="200"/>
        <inertia ixx="0.1" ixy="0.1" ixz="0.1" iyy="0.1" iyz="0.1" izz="0.1"/>
      </inertial>
    </link>
    
    <joint name="support_to_board" type="fixed">
      <parent link="support" />
      <child link="board" />
      <origin xyz="0 0.0 0.69" rpy="0.0 0.0 0.0" />
    </joint>
    
